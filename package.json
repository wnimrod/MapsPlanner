{
  "name": "mapsplanner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --skipLibCheck --noEmit && eslint . --ext ts,tsx --quiet",
    "lint:fix": "tsc --skipLibCheck --noEmit && eslint . --ext ts,tsx --quiet --fix",
    "preview": "vite preview",
    "build-messages": "formatjs extract --ignore=\"src/**/*.d.ts\" --out-file \"src/lang/en.json\" --id-interpolation-pattern '[sha512:contenthash:base64:6]' \"src/**/messages.ts\" --format simple"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@react-google-maps/api": "^2.19.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/validator": "^13.11.8",
    "axios": "^1.6.2",
    "axios-case-converter": "^1.1.0",
    "classnames": "^2.3.2",
    "formik": "^2.4.5",
    "google-map-react": "^2.2.1",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.5",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "swr": "^2.2.4",
    "use-async-memo": "^1.2.5",
    "use-places-autocomplete": "^4.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.4",
    "@formatjs/ts-transformer": "^3.13.9",
    "@storybook/builder-vite": "^7.6.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/google-map-react": "^2.1.10",
    "@types/jsonpath": "^0.2.4",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "stylelint": "^16.1.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.2"
  }
}
